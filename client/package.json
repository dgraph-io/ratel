    "webpack-visualizer-plugin": "^0.1.11",
    "whatwg-fetch": "^3.5.0",
    "workbox-webpack-plugin": "5.1.4"
  },
  "dependencies": {
    "@babel/node": "^7.12.10",
    "@fortawesome/fontawesome-free": "^5.15.1",
