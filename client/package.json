    "prettier": "^2.7.1",
    "promise": "^8.3.0",
    "prompts": "2.4.2",
    "raf": "^3.4.1",
    "react-dev-utils": "^11.0.4",
    "resolve": "1.22.1",
    "resolve-url-loader": "^3.1.4",
    "sass-loader": "^10.3.1",
    "semver": "7.3.8",
    "style-loader": "^2.0.0",
    "sw-precache-webpack-plugin": "^0.7.2",
    "terser-webpack-plugin": "4.2.3",
    "ts-pnp": "1.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.27.1",
    "webpack-dev-server": "^3.11.3",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-visualizer-plugin": "^0.1.11",
    "whatwg-fetch": "^3.6.2",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "dependencies": {
    "@babel/node": "^7.20.2",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "bootstrap": "^4.6.2",
    "browserslist": "^4.21.4",
    "classnames": "^2.3.2",
    "codemirror": "^5.65.9",
