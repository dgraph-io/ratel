{
  "name": "ratel-client",
  "version": "20.7.0",
  "description": "The user interface for interacting with Dgraph server",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dgraph-io/ratel.git"
  },
  "bugs": {
    "url": "https://github.com/dgraph-io/ratel/issues"
  },
  "homepage": "/",
  "publicUrl": "/",
  "cdnUrl": "https://d1ocqy7wcgv7nr.cloudfront.net/",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:prod": "node scripts/build.js",
    "build:local": "node scripts/build.js",
    "prettier": "prettier --write \"src/**/*.{js,jsx,mjs,json,scss}\"",
    "lint": "eslint \"src/**/*.{js,jsx,mjs}\"",
    "test": "node scripts/test.js",
    "test:watch": "node scripts/test.js --watch",
    "precommit": "lint-staged",
    "deepClean": "rm -rf node_modules yarn.lock package-lock.json"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,mjs}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ],
      "@babel/preset-react"
    ]
  },
  "browserslist": [
    ">2%",
    "last 3 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": "warn"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "tabWidth": 4,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.{json,yml}",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testResultsProcessor": "jest-teamcity",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "modulePaths": [
      "./src/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "bfj": "^7.0.2",
    "camelcase": "^6.2.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^4.1.0",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "5.1.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "eslint-webpack-plugin": "^2.4.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.6",
    "ignore-loader": "^0.1.2",
    "jest": "^26.6.3",
    "jest-circus": "26.6.3",
    "jest-resolve": "26.6.2",
    "jest-teamcity": "^1.9.0",
    "jest-watch-typeahead": "0.6.1",
    "jsdom": "^16.4.0",
    "leaflet": "^1.7.1",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^5.0.0",
    "object-assign": "^4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "pnp-webpack-plugin": "1.6.4",
    "postcss": "^8.1.10",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "5.0.2",
    "prettier": "^2.2.1",
    "promise": "^8.1.0",
    "prompts": "2.4.0",
    "puppeteer": "^1.18.1",
    "raf": "^3.4.1",
    "react-dev-utils": "^11.0.1",
    "resolve": "1.19.0",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^10.1.0",
    "semver": "7.3.4",
    "style-loader": "^2.0.0",
    "sw-precache-webpack-plugin": "^1.0.0",
    "terser-webpack-plugin": "4.2.3",
    "ts-pnp": "1.2.0",
    "url-loader": "^4.1.1",
