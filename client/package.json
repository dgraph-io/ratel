{
  "name": "ratel-client",
  "version": "0.0.1",
  "description": "The user interface for interacting with Dgraph server",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dgraph-io/ratel.git"
  },
  "bugs": {
    "url": "https://github.com/dgraph-io/ratel/issues"
  },
  "homepage": "https://github.com/dgraph-io/ratel#readme",
  "publicUrl": "/",
  "cdnUrl": "https://d1ocqy7wcgv7nr.cloudfront.net/",
  "scripts": {
    "start": "node scripts/start.js",
    "build:prod": "node scripts/build.prod.js",
    "build:local": "node scripts/build.local.js",
    "prettier": "prettier --write \"src/**/*.{js,jsx,mjs,json,scss}\"",
    "lint": "eslint \"src/**/*.{js,jsx,mjs}\"",
    "test": "node scripts/test.js --env=jsdom",
    "test:watch": "node scripts/test.js --env=jsdom --watch",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,mjs}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ],
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": "warn"
    }
  },
  "prettier": {
    "tabWidth": 4,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.{json,yml}",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "modulePaths": [
      "./src/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^6.1.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "2.0.0",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "husky": "^0.14.3",
    "ignore-loader": "^0.1.2",
    "jest": "^24.5.0",
    "jsdom": "^12.0.0",
    "lint-staged": "8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.7.2",
    "object-assign": "^4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.4.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.9.2",
    "promise": "^8.0.1",
    "raf": "^3.4.0",
    "react-dev-utils": "^6.1.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "1.1.2",
    "webpack": "^4.27.1",
    "webpack-dev-server": "^3.1.10",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-visualizer-plugin": "^0.1.11",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "bootstrap": "^4.1.3",
    "browserslist": "^4.1.1",
    "classnames": "^2.2.5",
    "codemirror": "^5.32.0",
    "codemirror-graphql": "^0.6.12",
    "core-js": "3",
    "crypto-js": "^3.1.9-1",
    "d3": "^5.7.0",
    "graphql": "0.13.0",
    "immer": "^1.8.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.4",
    "lodash-es": "^4.17.11",
    "pluralize": "^7.0.0",
    "randomcolor": "^0.5.3",
    "react": "^16.4.0",
    "react-bootstrap": "^1.0.0-beta.3",
    "react-clipboard.js": "^2.0.1",
    "react-data-grid": "^4.0.8",
    "react-dom": "^16.2.0",
    "react-draggable": "^3.0.5",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-timeago": "^4.1.9",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-persist": "^5.4.0",
    "redux-thunk": "^2.3.0",
    "screenfull": "^3.3.2",
    "url-search-params": "^0.10.0",
    "uuid": "^3.1.0"
  }
}
