{
  "name": "ratel-client",
  "version": "20.3.1-rc1",
  "description": "The user interface for interacting with Dgraph server",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dgraph-io/ratel.git"
  },
  "bugs": {
    "url": "https://github.com/dgraph-io/ratel/issues"
  },
  "homepage": "https://github.com/dgraph-io/ratel#readme",
  "publicUrl": "/",
  "cdnUrl": "https://d1ocqy7wcgv7nr.cloudfront.net/",
  "scripts": {
    "start": "node scripts/start.js",
    "build:prod": "node scripts/build.prod.js",
    "build:local": "node scripts/build.local.js",
    "prettier": "prettier --write \"src/**/*.{js,jsx,mjs,json,scss}\"",
    "lint": "eslint \"src/**/*.{js,jsx,mjs}\"",
    "test": "node scripts/test.js",
    "test:watch": "node scripts/test.js --watch",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,mjs}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{json,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ],
      "@babel/preset-react"
    ]
  },
  "browserslist": [
    ">2%",
    "last 3 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": "warn"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "tabWidth": 4,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.{json,yml}",
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testResultsProcessor": "jest-teamcity",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "modulePaths": [
      "./src/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^4.0.0",
    "css-loader": "^3.5.2",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^6.0.0",
    "fs-extra": "^9.0.0",
    "html-webpack-plugin": "^4.2.0",
    "husky": "^4.2.5",
    "ignore-loader": "^0.1.2",
    "jest": "^25.3.0",
    "jest-teamcity": "^1.7.0",
    "jsdom": "^16.2.2",
    "leaflet": "^1.6.0",
    "lint-staged": "^10.1.5",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "object-assign": "^4.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "prettier": "^2.0.5",
    "promise": "^8.1.0",
    "raf": "^3.4.1",
    "react-dev-utils": "^6.1.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "sw-precache-webpack-plugin": "^1.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.27.1",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-visualizer-plugin": "^0.1.11",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "@babel/node": "^7.8.7",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "bootstrap": "^4.4.1",
    "browserslist": "^4.12.0",
    "classnames": "^2.2.6",
    "codemirror": "^5.53.2",
    "codemirror-graphql": "^0.11.6",
    "core-js": "^3.6.5",
    "crypto-js": "^4.0.0",
    "d3": "^5.16.0",
    "dgraph-js-http": "^20.3.1-rc4",
    "graphql": "0.13.0",
    "immer": "^6.0.3",
    "jquery": "^3.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isempty": "^4.4.0",
    "lodash.merge": "^4.6.2",
    "memoize-one": "^5.1.1",
    "moment": "^2.24.0",
    "pluralize": "^8.0.0",
    "puppeteer": "^1.18.1",
    "randomcolor": "^0.5.4",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-clipboard.js": "^2.0.16",
    "react-data-grid": "^6.1.0",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.3.1",
    "react-highlight": "^0.12.0",
    "react-leaflet": "^2.6.3",
    "react-redux": "^7.2.0",
    "react-timeago": "^4.4.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "screenfull": "^5.0.2",
    "use-interval": "^1.2.1",
    "uuid": "^3.1.0"
  }
}
